{
  "manifest_version": 3,
  "name": "重复标签页清理器",
  "version": "4.2.0",
  "description": "检测并关闭重复的标签页，让你的浏览器更整洁",
  "permissions": [
    "tabs",
    "windows",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "重复标签页清理器"
  },
  "background": {
    "service_worker": "background.js"
  }
}